{
    "name": "arodu/cakelte",
    "description": "AdminLTE plugin for CakePHP",
    "type": "cakephp-plugin",
    "language": "PHP",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CakeLTE\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeLTE\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
