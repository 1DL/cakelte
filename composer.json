{
    "name": "arodu/cakelte",
    "description": "CakeLTE: AdminLTE plugin for CakePHP 4.x",
    "type": "cakephp-plugin",
    "language": "PHP",
    "license": "MIT",
    "homepage": "https://github.com/arodu/cakelte",
    "authors": [
      {
        "name": "Alberto G. Rodriguez U. <arodu.dev@gmail.com>",
        "homepage": "https://github.com/arodu"
      }
    ],
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CakeLTE\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeLTE\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
